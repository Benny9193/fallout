<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="/favicon-192x192.svg"/>
      <square150x150logo src="/favicon-192x192.svg"/>
      <square310x310logo src="/favicon-512x512.svg"/>
      <wide310x150logo src="/favicon-512x512.svg"/>
      <TileColor>#667eea</TileColor>
    </tile>
    <notification>
      <polling-uri src="https://fallout-web-app.com/notifications/feed1"/>
      <polling-uri2 src="https://fallout-web-app.com/notifications/feed2"/>
      <polling-uri3 src="https://fallout-web-app.com/notifications/feed3"/>
      <polling-uri4 src="https://fallout-web-app.com/notifications/feed4"/>
      <polling-uri5 src="https://fallout-web-app.com/notifications/feed5"/>
      <frequency>30</frequency>
      <cycle>1</cycle>
    </notification>
  </msapplication>
</browserconfig>
